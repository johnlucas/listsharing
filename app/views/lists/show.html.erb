<h1>List</h1>

<p>
  <b>Name</b><br>
  <%= @list.name %>
</p>

<p>
  <b>Items name</b><br>
  <%= @list.items_name %>
</p>

<div class="form-actions">
  <%= link_to 'Back', lists_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_list_path(@list), :class => 'btn' %>
  <%= link_to 'Delete', list_path(@list), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
